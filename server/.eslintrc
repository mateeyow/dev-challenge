{
  "extends": "eslint-config-airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "no-console": [
      "warn",
      {
        "allow": [
          "warn",
          "error",
          "info"
        ]
      }
    ],
    "import/prefer-default-export": "off"
  }
}